{
  "name": "nba-stats",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "watch-game": "node -r esm bin/watchGame.js",
    "poll-games": "node -r esm -r dotenv/config bin/pollGames.js",
    "backfill-games": "node -r esm -r dotenv/config bin/backfillGames.js",
    "season-stats": "node -r esm -r dotenv/config bin/teamSeasonStats.js",
    "update-schedule": "node -r esm -r dotenv/config bin/updateSchedule.js",
    "update-daily-scoreboard": "node -r esm -r dotenv/config bin/updateDailyScoreboard.js",
    "update-game": "node -r esm -r dotenv/config bin/updateGame.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c --bundleConfigAsCjs",
    "shell": "node",
    "dev:start:db": "cd dev && docker-compose -p nba-stats-dev up",
    "dev:setup:db": "./dev/setup-dynamodb.sh",
    "deploy:init": "terraform -chdir=./deployment init",
    "deploy:plan": "npm run build && terraform -chdir=./deployment plan",
    "deploy": "npm run build && terraform -chdir=./deployment apply -auto-approve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.212.0",
    "@aws-sdk/client-dynamodb": "^3.284.0",
    "@aws-sdk/client-s3": "^3.212.0",
    "@aws-sdk/client-sqs": "^3.215.0",
    "@aws-sdk/lib-dynamodb": "^3.284.0",
    "@peculiar/webcrypto": "^1.4.1",
    "esm": "^3.2.25",
    "inquirer": "^8.2.0",
    "isomorphic-unfetch": "^3.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@politico/eslint-config-interactives": "^1.0.2",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "rollup": "^3.4.0",
    "rollup-plugin-node-externals": "^5.0.2",
    "serverless": "^3.25.0",
    "yargs": "^17.6.2"
  }
}
